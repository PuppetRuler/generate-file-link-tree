{
  "name": "generate-file-link-tree",
  "displayName": "generate-file-link-tree",
  "description": "一键生成带有链接的markdown文档目录树",
  "publisher": "puppetmaster",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.generateFileTree",
        "title": "Generate File Tree"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "extension.generateFileTree",
          "group": "navigation@6",
          "when": "explorerResourceIsFolder"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.8",
    "@types/node": "20.x",
    "@types/vscode": "^1.80.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.11.1"
  },
  "dependencies": {
    "js-yaml": "^4.1.0"
  }
}
